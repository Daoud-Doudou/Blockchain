{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Node1",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "Blockchain_nodes.blockchain_node",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--port=5001"
            ],
            "jinja": true,
            "console": "integratedTerminal"
        }
    ]
}